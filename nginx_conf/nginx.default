server{
    listen 80;
    server_name 127.0.0.0;

    location / {
        proxy_pass http://127.0.0.1:8000;
        proxy_pass_header Server;
        proxy_set_header Host $http_host;
        proxy_redirect off;
    }

    location /static/ {
        alias /home/dockerized001/static/;
    }

    location /media/ {
        alias /home/dockerized001/media/;
    }

    
}